<h2><%= @mealplan.name %> Mealplan</h2>
<div class="row">
	<% @mealplan.days.each do |day| %>
		<div class="col-md-6 col-md-offset-1">
			<div class="well">
				<h4><%= day.name %>:</h4>

				<% day.recipes.each do |recipe| %>
					<%= link_to recipe.name, recipe %>
				<% end %>
			</div>
		</div>
	<% end %>
</div>